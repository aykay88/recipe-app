{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "sourceMap": true,
    "outDir": "dist",
    "allowJs": true,
    "noImplicitAny": false,
    "lib": [
      "es2015",
      "es2015.collection",
      "es2015.promise",
      "es2016",
      "dom"
    ]
  },
  "exclude": [
    "dist",
    "node_modules"
  ],
  "typeRoots": [
    "node_modules/@types",
    "types"
  ]
}